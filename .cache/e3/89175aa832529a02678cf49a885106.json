     background-color: $hover;\r\n            color: white;\r\n        }\r\n        &.selected {\r\n            background: rgba($hover, 0.4);\r\n\r\n            &::before {\r\n                content: \"✅\";\r\n                display: inline-block;\r\n                padding-right: 10px;\r\n            }\r\n        }\r\n    }\r\n    .choosely-option-container {\r\n        position: absolute;\r\n        top: 100%;\r\n        left: 0;\r\n        width: 100%;\r\n        max-height: 300px;\r\n        overflow-y: scroll;\r\n        transform: translateY(-50%) scaleY(0);\r\n        transition: transform 100ms ease;\r\n    }\r\n    &.c-opened {\r\n        .choosely-selected {\r\n            transition: $bg-transition;\r\n            background-color: darken($hover, 10);\r\n            color: white;\r\n\r\n            svg {\r\n                transform: translateX(3px) scaleY(0.9) rotateX(180deg);\r\n            }\r\n        }\r\n        .choosely-option-container {\r\n            transform: translateY(0) scaleY(1);\r\n        }\r\n    }\r\n\r\n    /* width */\r\n    ::-webkit-scrollbar {\r\n        width: 5px;\r\n    }\r\n\r\n    /* Track */\r\n    ::-webkit-scrollbar-track {\r\n        background: #eee;\r\n    }\r\n\r\n    /* Handle */\r\n    ::-webkit-scrollbar-thumb {\r\n        background: #ccc;\r\n    }\r\n\r\n    /* Handle on hover */\r\n    ::-webkit-scrollbar-thumb:hover {\r\n        background: $hover;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"66156fbb15e0d7b9b31fda066dec6189","cacheData":{"env":{}}}